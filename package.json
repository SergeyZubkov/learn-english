{
  "name": "heroku-cra-node",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "6.11.x"
  },
  "scripts": {
    "dev": "set NODE_ENV=development&&webpack -w",
    "start-dev": "set NODE_ENV=development&&webpack -w",
